{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "ng-tree-diagram": [
        "dist/ng-tree-diagram"
      ],
      "ng-tree-diagram/*": [
        "dist/ng-tree-diagram/*"
      ],
      "my-lib": [
        "dist/my-lib/my-lib",
        "dist/my-lib"
      ],
      "ng-tree-diagram2": [
        "dist/ng-tree-diagram2/ng-tree-diagram2",
        "dist/ng-tree-diagram2"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}